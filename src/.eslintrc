{
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "generator-star-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-unused-vars": [
      1,
      {
        "args": "none"
      }
    ],
    "max-len": [
      1,
      110,
      4,
      {
        "ignoreTemplateLiterals": true,
        "ignoreTrailingComments": true,
        "ignoreStrings": true,
        "ignoreUrls": true
      }
    ],
    "react/prop-types": 0,
    "react/jsx-no-bind": 1,
    "react/no-danger": 2,
    "key-spacing": [
      0,
      {
        "align": "value"
      }
    ],
    "no-multi-spaces": [2],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "quote-props": [
      2,
      "consistent-as-needed"
    ],
    "no-constant-condition": 0,
    "no-empty-function": 0,
    "import/no-unresolved": 0,
    "new-cap": 0,
    "no-debugger": 1,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "import/no-dynamic-require": 0
  },
  "plugins": [
    "prettier"
  ]
}
